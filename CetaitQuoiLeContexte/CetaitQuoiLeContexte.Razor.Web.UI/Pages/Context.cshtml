@page "{title}"
@model CetaitQuoiLeContexte.Razor.Web.UI.Pages.ContextModel
@{
    ViewData["Title"] = this.Model.Title + " - C'était quoi le contexte, déjà ?";
}

<div class="row">
    <h1>@Model.Current.Message</h1>
</div>

@if (!string.IsNullOrEmpty(Model.Current.From))
{
    <div class="row">
        <div class="col-lg-2">Contexte repéré :</div>
        <div class="col-lg-3">@Model.Current.From</div>
    </div>
}

