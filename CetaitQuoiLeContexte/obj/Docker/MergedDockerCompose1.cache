services:
  cetaitquoilecontexte.razor.web.ui:
    build:
      context: F:\Works\Perso\CetaitQuoiLeContexte.Github\CetaitQuoiLeContexte
      dockerfile: ../CetaitQuoiLeContexte.Razor.Web.UI/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: cetaitquoilecontexterazorwebui
    ports:
    - target: 80
  cetaitquoilecontexte.web.services.ui:
    build:
      context: F:\Works\Perso\CetaitQuoiLeContexte.Github\CetaitQuoiLeContexte
      dockerfile: CetaitQuoiLeContexte.Web.Services.UI/Dockerfile
      target: base
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: cetaitquoilecontextewebservicesui:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.1/CetaitQuoiLeContexte.Web.Services.UI.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    volumes:
    - F:\Works\Perso\CetaitQuoiLeContexte.Github\CetaitQuoiLeContexte\CetaitQuoiLeContexte.Web.Services.UI:/app:rw
    - C:\Users\Evan\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\Evan\.nuget\packages:/root/.nuget/packages:ro
  cetaitquoilecontexte.web.ui:
    build:
      context: F:\Works\Perso\CetaitQuoiLeContexte.Github\CetaitQuoiLeContexte
      dockerfile: ../CetaitQuoiLeContexte.Web.UI/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: cetaitquoilecontextewebui
    ports:
    - target: 80
version: '3.4'